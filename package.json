  "bugs": {
    "url": "https://github.com/elszczepano/node-text-chat/issues"
  },
  "homepage": "https://github.com/elszczepano/node-text-chat#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.1.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "pug": "^2.0.3",
    "socket.io": "^2.1.1"
